{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.9.5",
  "repository": {
    "url": "https://github.com/posva-sponsors/vue-termui.git",
    "type": "git"
  },
  "scripts": {
    "release": "node scripts/release.mjs",
    "dev": "pnpm run -r --filter './packages/{core,vite-plugin-vue-termui,cli}' dev",
    "build": "pnpm run -r --filter './packages/{core,vite-plugin-vue-termui,cli}' build",
    "play:dev": "pnpm run --filter  './packages/playground' dev",
    "lint": "prettier --check packages README.md",
    "lint:fix": "prettier --write packages README.md",
    "types": "pnpm run -r types",
    "test": "pnpm run --parallel -r test --coverage",
    "docs": "pnpm run -r dev --filter ./packages/docs",
    "docs:build": "pnpm -r --filter ./packages/docs run build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/runtime-core": "^3.2.39",
    "c8": "^7.11.2",
    "chalk": "^5.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "execa": "^6.1.0",
    "globby": "^13.1.1",
    "minimist": "^1.2.6",
    "p-series": "^3.0.0",
    "prettier": "^2.6.2",
    "semver": "^7.3.7",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.2",
    "vitest": "^0.23.2",
    "vue": "^3.2.39",
    "vue-tsc": "^0.40.13"
  }
}
