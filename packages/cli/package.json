{
  "name": "@vue-termui/cli",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "0.0.15",
  "scripts": {
    "stub": "unbuild --stub",
    "build": "tsup",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @vue-termui/cli -r 1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "vtui": "./vtui.mjs"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "vtui.mjs"
  ],
  "keywords": [
    "vue",
    "term",
    "ui",
    "cli",
    "vue-termui-cli",
    "vue-termui"
  ],
  "funding": "https://github.com/vue-terminal/vue-termui?sponsor=1",
  "license": "MIT",
  "author": "Eduardo San Martin Morote (https://esm.dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-terminal/vue-termui.git",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/vue-terminal/vue-termui/issues"
  },
  "homepage": "https://github.com/vue-terminal/vue-termui#readme",
  "dependencies": {
    "picocolors": "^1.0.0",
    "vite-node": "^0.24.3",
    "ws": "^8.5.0"
  },
  "peerDependencies": {
    "vite": "^3.1.3",
    "vite-plugin-vue-termui": ">=0.0.10",
    "vue-termui": ">=0.0.13"
  },
  "devDependencies": {
    "@types/ws": "^8.5.3",
    "cac": "^6.7.12",
    "fast-glob": "^3.2.12",
    "pathe": "^0.3.9",
    "rimraf": "^3.0.2",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-vue-termui": "workspace:*",
    "vue-termui": "workspace:*"
  },
  "unbuild": {
    "entries": [
      "src/cli",
      "src/index"
    ],
    "rollup": {
      "emitCJS": true
    },
    "clean": true
  }
}
